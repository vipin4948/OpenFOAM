FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/sphere";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];


internalField   uniform 1;

boundaryField
{
    sphere_to_air
    {
        type            calculated;
        value           uniform 0;
    }
}


