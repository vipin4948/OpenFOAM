FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/sphere";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 0 1 0 0 0 ];

internalField   uniform 500;

boundaryField
{
    sphere_to_air
    {
        type            compressible::turbulentTemperatureCoupledBaffleMixed;
        value           uniform 500;
        Tnbr            T;
        kappaMethod     solidThermo;
    }
}


