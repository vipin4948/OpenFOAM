1
(
    sphere_to_air
    {
        type            mappedWall;
        inGroups        1 ( wall );
        nFaces          1536;
        startFace       8400;
        sampleMode      nearestPatchFace;
        sampleRegion    air;
        samplePatch     air_to_sphere;
    }

