FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "650/water";
    object      rho;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 1000;

boundaryField
{
    front
    {
        type            calculated;
        value           uniform 1000;
    }
    back
    {
        type            calculated;
        value           uniform 1000;
    }
    left
    {
        type            calculated;
        value           uniform 1000;
    }
    right
    {
        type            calculated;
        value           uniform 1000;
    }
    water_to_heater
    {
        type            calculated;
        value           uniform 1000;
    }
}


