FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    wall
    {
       type            noSlip;
    }

    inlet
    {
        type            zeroGradient;
    }

   outlet
    {
        type            fixedValue;
        value           uniform (0 0.02 0);
    }

    inlet2
    {
        type            zeroGradient;
        
    }


    frontAndBackPlanes
    {
        type            empty;
    }
}

